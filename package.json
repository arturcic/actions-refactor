{
    "name": "actions2",
    "version": "0.0.0",
    "type": "module",
    "module": "ES2022",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build:tools": "vite build --config src/tools/vite.config.ts",
        "build:agent:fake": "vite build --config src/agents/vite.config.ts --mode fake",
        "build:agent:azure": "vite build --config src/agents/vite.config.ts --mode azure",
        "build:agent:github": "vite build --config src/agents/vite.config.ts --mode github",
        "build:agents": "run-p build:agent:fake build:agent:azure build:agent:github",
        "run:fake:gitversion": "node dist/tools/gitversion.js --buildAgent fake",
        "run:azure:gitversion": "node dist/tools/gitversion.js --buildAgent azure",
        "run:github:gitversion": "node dist/tools/gitversion.js --buildAgent github",
        "run:fake:gitreleasemanager": "node dist/fake/gitreleasemanager.js",
        "run:azure:gitreleasemanager": "node dist/azure/gitreleasemanager.js",
        "run:github:gitreleasemanager": "node dist/github/gitreleasemanager.js",
        "test:tools": "vitest --run src/__tests__/tools --config src/__tests__/vitest.config.ts",
        "test:agents": "vitest --run src/__tests__/agents --config src/__tests__/vitest.config.ts",
        "build": "run-p build:tools build:agents",
        "test": "run-p test:tools test:agents",
        "lint": "eslint --ext .ts src",
        "format": "npx prettier src/**/*.ts --write"
    },
    "devDependencies": {
        "@types/node": "^20.11.17",
        "@types/semver": "^7.5.7",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "dotenv": "^16.4.4",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-github": "^4.10.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vitest": "^0.3.22",
        "npm-run-all2": "^6.1.2",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3",
        "vite": "^5.1.1",
        "vite-node": "^1.2.2",
        "vite-plugin-dynamic-import-module": "^0.2.1",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.2.2"
    },
    "dependencies": {
        "@actions/core": "^1.10.1",
        "@actions/exec": "^1.1.1",
        "@actions/io": "^1.1.3",
        "@actions/tool-cache": "^2.0.1",
        "azure-pipelines-task-lib": "^4.9.0",
        "azure-pipelines-tool-lib": "^2.0.7",
        "semver": "^7.6.0",
        "shelljs": "Everspace/shelljs#b364c45"
    },
    "overrides": {
        "shelljs": "$shelljs"
    }
}
