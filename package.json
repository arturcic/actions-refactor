{
    "name": "actions2",
    "version": "0.0.0",
    "type": "module",
    "module": "es2022",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build:azure:gitversion": "vite build -c src/tools/gitversion/vite.config.ts --mode azure",
        "build:azure:gitreleasemanager": "vite build -c src/tools/gitreleasemanager/vite.config.ts --mode azure",
        "build:github:gitversion": "vite build -c src/tools/gitversion/vite.config.ts --mode github",
        "build:github:gitreleasemanager": "vite build -c src/tools/gitreleasemanager/vite.config.ts --mode github",

        "run:azure:gitversion": "node dist/azure/gitversion.js -n Artur -c",
        "run:azure:gitreleasemanager": "node dist/azure/gitreleasemanager.js -n Artur -c",
        "run:github:gitversion": "node dist/github/gitversion.js -n Artur -c",
        "run:github:gitreleasemanager": "node dist/github/gitreleasemanager.js -n Artur -c"
    },
    "devDependencies": {
        "@types/node": "^20.5.9",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vite-node": "^0.34.3",
        "vite-plugin-dts": "^3.5.3",
        "vitest": "^0.34.3"
    }
}
